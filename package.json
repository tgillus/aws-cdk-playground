{
  "name": "aws-cdk-playground",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "aws-cdk-playground": "bin/aws-cdk-playground.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "clean": "del-cli lib coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@release-it/conventional-changelog": "8.0.1",
    "@types/aws-lambda": "8.10.143",
    "@types/node": "22.4.0",
    "@typescript-eslint/eslint-plugin": "8.1.0",
    "@typescript-eslint/parser": "8.1.0",
    "@vitest/coverage-v8": "2.0.5",
    "aws-cdk": "2.152.0",
    "del-cli": "5.1.0",
    "esbuild": "0.23.1",
    "eslint": "9.9.0",
    "husky": "9.1.4",
    "lint-staged": "15.2.9",
    "prettier": "3.3.3",
    "release-it": "17.6.0",
    "tsx": "4.17.0",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.632.0",
    "@aws-sdk/client-ec2": "3.632.0",
    "@aws-sdk/client-s3": "3.633.0",
    "@aws-sdk/credential-provider-node": "3.632.0",
    "@aws-sdk/lib-dynamodb": "3.632.0",
    "@opensearch-project/opensearch": "2.11.0",
    "@sindresorhus/is": "7.0.0",
    "aws-cdk-lib": "2.152.0",
    "constructs": "10.3.0",
    "date-fns": "3.6.0",
    "dotenv": "16.4.5",
    "env-var": "7.5.0",
    "got": "14.4.2",
    "nanoid": "5.0.7",
    "pkg-dir": "8.0.0",
    "purify-ts": "2.1.0",
    "remeda": "2.11.0",
    "source-map-support": "0.5.21",
    "ts-pattern": "5.3.1"
  }
}
