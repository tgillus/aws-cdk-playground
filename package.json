{
  "name": "aws-cdk-playground",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "aws-cdk-playground": "bin/aws-cdk-playground.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "clean": "del-cli lib coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky install",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@release-it/conventional-changelog": "7.0.1",
    "@types/aws-lambda": "8.10.119",
    "@types/node": "20.6.0",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "@vitest/coverage-v8": "0.34.4",
    "aws-cdk": "2.95.1",
    "del-cli": "5.1.0",
    "esbuild": "0.19.2",
    "eslint": "8.49.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "release-it": "16.1.5",
    "tsx": "3.12.8",
    "typescript": "5.2.2",
    "vitest": "0.34.4"
  },
  "dependencies": {
    "@aws-sdk/credential-provider-node": "3.409.0",
    "@opensearch-project/opensearch": "2.3.1",
    "@sindresorhus/is": "6.0.0",
    "aws-cdk-lib": "2.95.1",
    "constructs": "10.2.70",
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "env-var": "7.4.1",
    "got": "13.0.0",
    "pkg-dir": "7.0.0",
    "purify-ts": "2.0.1",
    "source-map-support": "0.5.21",
    "ts-pattern": "5.0.5"
  }
}
