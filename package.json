{
  "name": "aws-cdk-playground",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "aws-cdk-playground": "bin/aws-cdk-playground.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "clean": "del-cli lib coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky install",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
    "@release-it/conventional-changelog": "8.0.1",
    "@types/aws-lambda": "8.10.137",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "@vitest/coverage-v8": "1.5.0",
    "aws-cdk": "2.138.0",
    "del-cli": "5.1.0",
    "esbuild": "0.20.2",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "release-it": "17.2.0",
    "tsx": "4.7.2",
    "typescript": "5.4.5",
    "vitest": "1.5.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.556.0",
    "@aws-sdk/client-ec2": "3.557.0",
    "@aws-sdk/client-s3": "3.556.0",
    "@aws-sdk/credential-provider-node": "3.556.0",
    "@aws-sdk/lib-dynamodb": "3.556.0",
    "@opensearch-project/opensearch": "2.7.0",
    "@sindresorhus/is": "6.2.0",
    "aws-cdk-lib": "2.138.0",
    "constructs": "10.3.0",
    "date-fns": "3.6.0",
    "dotenv": "16.4.5",
    "env-var": "7.4.1",
    "got": "14.2.1",
    "nanoid": "5.0.7",
    "pkg-dir": "8.0.0",
    "purify-ts": "2.0.3",
    "remeda": "1.61.0",
    "source-map-support": "0.5.21",
    "ts-pattern": "5.1.1"
  }
}
